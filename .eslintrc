{
	"parser": "babel-eslint",
	"extends": ["airbnb-base", "prettier"],
	"rules": {
		"arrow-body-style": 0,
		"arrow-parens": 0,
		"consistent-return": 0,
		"func-names": 0,
		"function-paren-newline": 0,
		"no-plusplus": 0,
		"no-use-before-define": 0,
		"no-underscore-dangle": 0,
		"object-curly-newline": 0,
		"prefer-destructuring": 0,
		"prettier/prettier": "error",
		"react/jsx-filename-extension": 0
	}
}
